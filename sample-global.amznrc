# Sample "global" configuration for amzn, to be stored in ~/.amznrc

export AWS_DEFAULT_REGION=eu-west-1
export AWS_ACCESS_KEY_ID=your-access-key-id
export AWS_SECRET_ACCESS_KEY=your-secret-access-key

export AMZN_BASE_DIR=~/path/to/amzn-base
export AMZN_SHIP_DIR=~/path/to/amzn-ship

export RELEASE_BUCKET=s3://your-bucket-for-amzn-releases
export ZONES="eu-west-1a, eu-west-1b, eu-west-1c"

export RELEASE_PREFIX=r
export ASG_COOLDOWN=10
